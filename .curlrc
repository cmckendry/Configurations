# Disguise as IE 9 on Windows 7
user-agent = "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_9_3) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/34.0.1847.76 Safari/537.36 Carter/McKendry"
# When following a redirect, automatically set the previous URL as referer
referer = ";auto"
# Add a newline at the end of the response, so it doesn't ruin your bash day
write-out = "\n"

